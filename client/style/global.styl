html
  box-sizing border-box
  font-family "IM-Fell-english", sans-serif
  font-size 16px
  line-height 1.3
*
  box-sizing inherit
  margin 0
  padding 0
  font-size 1em
  font-weight normal
  font-style normal
em, i
  font-style italic
strong, b
  font-weight bold
body
  background #333
  color wheat
@font-face {
  font-family: "IM-Fell-english";
  src: local("IM-Fell-english"), url("../fonts/fonts/Latin/IM-Fell-english.sc.ttf") format("truetype");
  font-weight: normal;
  font-style: normal;
}
.heading
  headingSizes = { '1': 45px, '2': 40px, '3': 35px, '4': 30px, '5': 25px, '6': 20px }
  for key, value in headingSizes
    &{key}
      font-size value
